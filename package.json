{
  "name": "pibox-host",
  "version": "1.37.0",
  "private": true,
  "bin": "server.js",
  "scripts": {
    "dev": "NEXT_TELEMETRY_DISABLED=1 NODE_ENV=development node server.js",
    "build": "rm -rf .next && NEXT_TELEMETRY_DISABLED=1 next build && tar $(cat package.json | jq .version)",
    "start": "NEXT_TELEMETRY_DISABLED=1 next start -p 80",
    "lint": "NEXT_TELEMETRY_DISABLED=1 next lint"
  },
  "dependencies": {
    "@fortawesome/fontawesome-svg-core": "^6.5.1",
    "@fortawesome/free-regular-svg-icons": "^6.5.1",
    "@fortawesome/free-solid-svg-icons": "^6.5.1",
    "@fortawesome/react-fontawesome": "^0.2.0",
    "autoprefixer": "10.4.14",
    "canvas": "^2.11.2",
    "eslint": "8.40.0",
    "eslint-config-next": "13.4.2",
    "file-type": "^18.5.0",
    "next": "13.4.2",
    "postcss": "8.4.23",
    "randomcolor": "^0.6.2",
    "raw-body": "^2.5.2",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "sharp": "^0.32.1",
    "tailwindcss": "3.3.2"
  },
  "prettier": {
    "printWidth": 180,
    "singleQuote": true,
    "semi": false
  }
